# Custom Aliases
# Author: Joel Turbi

# Run up.sh shell script
alias up='~/../../mnt/e/Bash/Scripts_2.0/./up.sh'
# Run compile.sh shell script
alias exe='~/../../mnt/e/Bash/Scripts_2.0/./compile.sh'
# cd into CS246 
alias cs='cd /mnt/e/Bash/CS246'
# cd into my personal Scripts
alias script='cd /mnt/e/Bash/Scripts_2.0'
# cd into home
alias home='cd ~/'
# cd into local repos
alias work='cd /mnt/e/Bash'
# Shows up history of commands
alias hi='history'
# Removes all files ending with the extention .out
alias rem='exe --clean'
# Starts Window's file explorer in current directory
alias open='explorer.exe .'
